<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_load">

    <fragment
        android:id="@+id/navigation_load"
        android:name="com.tjr.wordsearchsolver.ui.load.LoadFragment"
        android:label="@string/title_load"
        tools:layout="@layout/fragment_load" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.tjr.wordsearchsolver.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.tjr.wordsearchsolver.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>